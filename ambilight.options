Example.name        max_length:40

// 4-tuple of IP components
Message.Config.ipv4         max_length:15

// With 3 bytes (RGB) per LED, this gives us enough for 256 LEDs
Message.Data.led_data       max_size:768

// We support FastLED palettes with up to 16 entries (each is 4 bytes)
Message.Data.led_palette    max_size:64